                var active_class = current_page == (i+1) || (!current_page && i==0) ? ' active' : '';
                var btn = '<a href="#" class="btn btn-secondary paginator page'+ (i+1) + active_class +'" data-page="'+ (i+1) +'">'+ (i+1) +'</a>';
