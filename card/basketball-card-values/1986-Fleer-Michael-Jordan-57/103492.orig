    function trimCardImage() {

        $('.mainwrp .content-box').removeAttr('style');
        $('.mainwrp .picbox img').removeAttr('style');

        var realImgWidth = 438;
        var maxWidth  = 438;
        var maxHeight = 386;
        var imgWidth  = $('.page-carddetail .picbox img').removeAttr('style').width();
        var imgHeight = $('.page-carddetail .picbox img').height();

        //if image is bigger then 438px set width to 438px
        //if image is smaller then 438 set width to real width
        if(imgWidth >= maxWidth || imgWidth == 0)
            var realImgWidth = maxWidth;
        else
            var realImgWidth = imgWidth;

        //if height is too big set to 386px and set width to realWidth
        if(imgHeight > maxHeight) {
        	$('.page-carddetail .picbox img').css('width', 'auto').height(maxHeight);
        	var realImgWidth = $('.page-carddetail .picbox img').width();
        }

        //adjust height if width is under maxWidth
        if(imgWidth < 438) {
          $('.page-carddetail .picbox img').css('width', 'auto').height('auto');
        }

        //adjust size if width is larger than maxWidth
        if(imgWidth > 438) {
          $('.page-carddetail .picbox img').css('width', maxWidth).height('auto');
        }

        //there is less space in this resolution range so make image smaller
        if($(window).width() > 1184 && $(window).width() < 1425) {
            var realImgWidth = 280;
        }

        if($(window).width() > 1330 && $(window).width() < 1425) {
            var realImgWidth = 350;
        }

        if($(window).width() <= 866) {
            $('.mainwrp').addClass('mobile');
            $('.mainwrp .content-box').removeAttr('style');
            $('.mainwrp .picbox').removeAttr('style');
            $('.mobile .picbox img').css('max-height', maxHeight).width('auto').css('border', '1px solid #E0E0E0');
        } else {

            $('.mainwrp').removeClass('mobile');
            $('.mainwrp .content-box').removeAttr('style');
            $('.mainwrp .picbox img').removeAttr('style').css('max-width', realImgWidth).css('border', '1px solid #E0E0E0');

            if($(window).width() <= 768)
                $('.mainwrp .content-box').css('padding-left', '15px');

            var wrp = $('.mainwrp').width();
            var w_img = $('.mainwrp .picbox img').width();
            var w_content = wrp - w_img;
            $('.mainwrp .content-box').width(w_content);
        }
    }

